{
    "name": "petelashin/filter-components",
    "description": "Laravel Package to extend Spatie Query Builder filtering",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Petro Lashyn",
            "email": "petelashin@gmail.com"
        }
    ],
    "homepage": "https://github.com/petelashin/spatie-filter-components",
    "keywords": ["Laravel", "Spatie", "Filters"],
    "require": {
        "php": ">=7.4.0",
        "spatie/laravel-query-builder": "^3.0",
        "laravel/framework": "^8.65"
    },
    "autoload": {
        "psr-4": {
            "PL\\FilterComponents\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PL\\FilterComponents\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}